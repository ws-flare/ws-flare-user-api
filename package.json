{
  "name": "ws-flare-user-api",
  "version": "1.0.0",
  "description": "ws-flare-user-api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "nyc --reporter=text-summary --reporter=html cross-env JWT_SECRET=test lb-mocha --allow-console-logs \"dist/test\"",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test",
    "kube": "telepresence --swap-deployment test-unicron-user-api --expose 3000 --run cross-env PORT=3000 yarn start",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.0.2",
    "@loopback/boot": "^1.0.2",
    "@loopback/context": "^1.0.0",
    "@loopback/core": "^1.0.0",
    "@loopback/openapi-v3": "^1.0.2",
    "@loopback/repository": "^1.0.2",
    "@loopback/rest": "^1.1.0",
    "@loopback/service-proxy": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "jsonwebtoken": "^8.3.0",
    "loopback-connector-mysql": "^5.3.1",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.0.0",
    "@loopback/testlab": "^1.0.0",
    "@types/node": "^10.11.2",
    "@types/passport": "^0.4.6",
    "@types/passport-http": "^0.3.6",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "nyc": "^13.3.0"
  }
}
